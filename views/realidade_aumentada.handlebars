   <body style="margin : 0px; overflow: hidden; font-family: Monospace;">
   
      <a-scene embedded vr-mode-ui="enabled: false" arjs="trackingMethod: best; sourceType: webcam; detectionMode: mono_and_matrix; matrixCodeType: 3x3; debugUIEnabled: false; changeMatrixMode: cameraTransformMatrix">
         
            
            
           
         
         <a-entity camera>

            
             <a-marker type="barcode" data-code="1" value="1" registerevents="true">
             <a-entity collada-model="url(buzato.dae)"   scale="0.3 0.3 0.3 0.3" transparent> </a-entity>
                 
            </a-marker>







            <a-marker class="game-marker" id="marker2" type="barcode" data-code="2" value="2" registerevents="true">
               <a-entity collada-model="url(model.dae)"  rotation="-90 0 0"   scale="8.2 8.2 8.2 8.2" transparent> </a-entity>
            </a-marker>
            <a-marker class="game-marker" id="marker4" type="barcode" data-code="4" value="4" registerevents="true">
           
               <a-entity  obj-model="obj: url(Bar_chair_2.obj); mtl: url(Bar_chair_2.mtl)" > </a-entity>
            </a-marker>
            <a-marker class="game-marker" id="marker5" type="barcode" data-code="5" value="5" registerevents="true">
               <a-image src="./images/clues/vida.png" width="3" height="3" rotation="-90 0 0" position="0 0 0 -5" scale="2 2 2 2" transparent></a-image>
            </a-marker>
            <a-marker class="game-marker" id="marker6" type="barcode" data-code="6" value="6" registerevents="true">
